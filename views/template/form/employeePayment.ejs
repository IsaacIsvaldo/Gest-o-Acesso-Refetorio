  <script>
    const domain = document.getElementById("domainID").value;
    function update(){
      const employeeCod= document.getElementById("employeeCod").value
const checkedRadioButton = document.querySelector('input[name="statusPayment"]:checked');
if (checkedRadioButton) {
  const statusPayment = checkedRadioButton.value;
 
    console.log(statusPayment);
    data={
      statusPayment:statusPayment,
      employeeCod:employeeCod
    }

    fetch(`${domain}/employeePaymentRegister`, {
        method: 'POST',
        headers: {
        "Content-Type": "application/json", // Tipo de conteúdo do corpo da requisição
      },
      body: JSON.stringify(data),
    })
    .then(response => {
        if (!response.ok) {
            throw new Error('Ocorreu um erro .');
        }
        return response.json();
    })
    .then(data => {
        if(!data.error){
          window.location ="/controlRegisterEmployee"
        }
    })
    .catch(error => {
        // Lidar com erros 
        console.error('Erro:', error);
    });
} else {
  console.log('Nenhum radio button está marcado.');
}

    }
    </script>


