    <script>

      var resposta = null
      function ValidarExt() {
          var customFile = document.getElementById('customFile');
          var customRuta = customFile.value;
          var customPermitidas = /(.XLSX|.xlsx)$/i;
          var btn = document.getElementById('btnupload');
          if (!customPermitidas.exec(customRuta)) {
            Swal.fire({
              icon: 'error',
              title: 'Oops...',
              text: 'Extensão não permitido!',
    
            })
            customFile.value = '';
            btn.setAttribute('hidden', true);
            return false
          }else{
            btn.removeAttribute('hidden');
          }
        }
      
     function enviar() {
      var card1 = document.getElementById('card1');
      var card2 = document.getElementById('card2');
      var info = document.getElementById('info');
    const form = document.getElementById('formUpload');
    const formData = new FormData(form);
    console.log(formData);

    fetch('http://localhost:8000/uploadFile', {
        method: 'POST',
        body: formData
    })
    .then(response => {
        if (!response.ok) {
            throw new Error('Ocorreu um erro ao enviar a imagem.');
        }
        return response.json();
    })
    .then(data => {
      card1.setAttribute('hidden', true);
      card2.removeAttribute('hidden');
      info.innerHTML=data.filename
      resposta =data.filename
        console.log('Resposta do servidor:', data);
    })
    .catch(error => {
        // Lidar com erros
        console.error('Erro:', error);
    });
}
     

function adicionar(){
   console.log(resposta)
  fetch(`http://localhost:8000/InsertDataFile/${resposta}`, {
        method: 'GET'
    })
    .then(response => {
        if (!response.ok) {
            throw new Error('Ocorreu um erro.');
        }
        return response.json();
    })
    .then(data => {
        console.log('Resposta do servidor:', data);
        window.location ="/ImportFile"
    })
    .catch(error => {
        // Lidar com erros
        console.error('Erro:', error);
    });
}
      </script>
  
